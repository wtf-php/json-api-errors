{
    "name": "wtfphp/json-api-errors",
    "description": "json-api-error middleware package",
    "homepage": "https://wtf-php.github.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Pichler",
            "email": "admin@fetzi.dev",
            "role": "Maintainer"
        },
        {
            "name": "Daniela Huber",
            "email": "huber.daniela@live.at",
            "role": "Maintainer"
        },
        {
            "name": "Cathrin Auer",
            "email": "cathrin.auer@gmx.at",
            "role": "Maintainer"
        },
        {
            "name": "Daniel Wittberger",
            "email": "daniel@wittberger.net",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.4",
        "lukasoppermann/http-status": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "tightenco/collect": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/var-dumper": "^5.0",
        "slim/psr7": "^1.1",
        "slim/slim": "4.*"
    },
    "autoload": {
        "psr-4": {
            "WtfPhp\\JsonApiErrors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WtfPhp\\JsonApiErrors\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit",
        "lint": "vendor/bin/phpcs --standard=PSR12 --colors -w  ./src"
    },
    "config": {
        "sort-packages": true,
        "process-timeout": 0
    }
}
